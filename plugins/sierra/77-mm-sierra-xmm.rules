# do not edit this file, it will be overwritten on update

ACTION!="add|change|move", GOTO="mm_sierra_xmm_port_types_end"
SUBSYSTEMS=="usb", ATTRS{idVendor}=="1519", GOTO="mm_sierra_xmm_port_types"
GOTO="mm_sierra_xmm_port_types_end"

LABEL="mm_sierra_xmm_port_types"
SUBSYSTEMS=="usb", ATTRS{bInterfaceNumber}=="?*", ENV{.MM_USBIFNUM}="$attr{bInterfaceNumber}"

# Sierra HL758x
ATTRS{idVendor}=="1519", ATTRS{idProduct}=="0443", ENV{.MM_USBIFNUM}=="06", ENV{ID_MM_XMM_NCM_ID}="0"
ATTRS{idVendor}=="1519", ATTRS{idProduct}=="0443", ENV{.MM_USBIFNUM}=="08", ENV{ID_MM_DEVICE_IGNORE}="1"
ATTRS{idVendor}=="1519", ATTRS{idProduct}=="0443", ENV{.MM_USBIFNUM}=="0a", ENV{ID_MM_DEVICE_IGNORE}="1"
ATTRS{idVendor}=="1519", ATTRS{idProduct}=="0443", ENV{.MM_USBIFNUM}=="0c", ENV{ID_MM_DEVICE_IGNORE}="1"
ATTRS{idVendor}=="1519", ATTRS{idProduct}=="0443", ENV{.MM_USBIFNUM}=="00", ENV{ID_MM_XMM_ACM_ID}="0"
# Ignore intel trace ACM port
ATTRS{idVendor}=="1519", ATTRS{idProduct}=="0443", ENV{.MM_USBIFNUM}=="02", ENV{ID_MM_DEVICE_IGNORE}="1"
ATTRS{idVendor}=="1519", ATTRS{idProduct}=="0443", ENV{.MM_USBIFNUM}=="04", ENV{ID_MM_XMM_ACM_ID}="2"

LABEL="mm_sierra_xmm_port_types_end"
